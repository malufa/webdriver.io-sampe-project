{
  "name": "webdriver.io-sampe-project.git",
  "description": "sample automation",
  "version": "1.0.0",
  "scripts": {
    "test": "node_modules/.bin/wdio config/wdio.conf.chrome.head.js --suite testcase",
    "test-headless": "node_modules/.bin/wdio config/wdio.conf.chrome.headless.js --suite testcase",
    "webdriver-install": "node_modules/.bin/selenium-standalone install --version=3.9.1 --drivers.chrome.version=2.35 --drivers.chrome.arch=x64",
    "webdriver-start": "node_modules/.bin/selenium-standalone start --version=3.9.1 --drivers.chrome.version=2.35 --drivers.chrome.arch=x64"
  },
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^4.1.2",
    "cucumber": "^4.0.0",
    "deepmerge": "^2.0.1",
    "eslint-config-prettier": "^2.1.1",
    "junit-viewer": "4.11.1",
    "request": "^2.81.0",
    "require-dir": "^1.0.0",
    "selenium-standalone": "^6.12.0",
    "wdio-cucumber-framework": "^1.1.0",
    "wdio-junit-reporter": "^0.3.1",
    "wdio-screenshot": "^0.6.0",
    "wdio-selenium-standalone-service": "^0.0.9",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.10.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^3.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
